{
  "name": "slideshow-dist",
  "version": "1.0.0",
  "description": "Slideshow distribution",
  "main": "index.js",
  "bin": "index.cjs",
  "type": "module",
  "scripts": {
    "start": "node . -m md",
    "publish": "node deploy",
    "deploy": "node deploy --push"
  },
  "publishDirs": [
    [
      [
        "dist/*",
        "public/build"
      ],
      {
        "up": 1
      }
    ],
    [
      [
        "md/*",
        "public"
      ],
      {}
    ],
    [
      [
        "md/images/*",
        "public/images"
      ],
      {
        "up": 2
      }
    ],
    [
      [
        "md/assets/*",
        "public/assets"
      ],
      {
        "up": 2
      }
    ],
    [
      [
        "dev/*",
        "public"
      ],
      {
        "up": 1
      }
    ]
  ],
  "author": "Khan Kibria",
  "license": "Use for slide presentation allowed",
  "homepage": "https://kkibria.github.io/slideshow-dist/",
  "dependencies": {
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "directory-tree": "^3.2.1",
    "livereload": "^0.9.3",
    "nodegit": "^0.27.0",
    "open": "^8.4.0",
    "pkg": "^5.5.2",
    "polka": "^0.5.2",
    "port-authority": "^1.1.2",
    "sirv": "^1.0.14"
  },
  "devDependencies": {
    "stdio": "^2.1.1"
  },
  "pkg": {
    "assets": [
      "dist/*",
      "dev/*"
    ],
    "outputPath": "ss-bin"
  }
}
